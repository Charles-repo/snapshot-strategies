{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "address": {
          "type": "string",
          "title": "veGNOME Token Contract address",
          "examples": ["e.g. 0x13c3b73f24E30B364fA7A5eDff4a752f7771696e"]
        },
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. veGNOME"],
          "maxLength": 16
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": ["e.g. 18"]
        }
      },
      "required": ["address", "decimals"],
      "additionalProperties": false
    }
  }
}
